{
  "manifest_version": 3,
  "name": "Highlight & Search: Maps, Wikipedia, YouTube and More",
  "version": "1.0",
  "permissions": ["contextMenus", "activeTab", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "logos/icon_16x16.png",
    "48": "logos/icon_48x48.png",
    "128": "logos/icon_128x128.png"
  },
  "options_page": "options.html",
  "action": {
    "default_title": "Highlight & Search Settings",
    "default_popup": "options.html"
  }
}
